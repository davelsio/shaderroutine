{
  "name": "shaderroutine",
  "private": true,
  "main": "index.ts",
  "version": "0.1.0",
  "scripts": {
    "start": "NODE_ENV=development expo start",
    "android": "NODE_ENV=development expo run:android",
    "android:build:preview": "eas build --profile preview --platform android",
    "android:build:prod": "eas build --profile production --platform android",
    "ios": "NODE_ENV=development expo run:ios",
    "ios:build:preview": "eas build --profile preview --platform ios",
    "ios:build:prod": "eas build --profile production --platform ios",
    "eas:update:preview": "eas update --channel preview --clear-cache --environment preview && pnpx sentry-expo-upload-sourcemaps dist",
    "eas:update:prod": "eas update --channel production --clear-cache --environment production && pnpx sentry-expo-upload-sourcemaps dist",
    "lint": "eslint . --fix",
    "format": "prettier --check --write --no-error-on-unmatched-pattern ./**/*.{ts,tsx,md} ./**/*.{ts,tsx,md}",
    "test": "jest",
    "expo:check": "expo install --check",
    "expo:doctor": "pnpx expo-doctor --verbose",
    "expo:install": "expo install --pnpm",
    "expo:prebuild": "expo prebuild --clean",
    "tsc:check": "tsc --noEmit",
    "tsc:config": "tsc --showConfig"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  },
  "reanimated": {
    "staticFeatureFlags": {
      "ANDROID_SYNCHRONOUSLY_UPDATE_UI_PROPS": true,
      "DISABLE_COMMIT_PAUSING_MECHANISM": true,
      "EXPERIMENTAL_CSS_ANIMATIONS_FOR_SVG_COMPONENTS": false,
      "IOS_DYNAMIC_FRAMERATE_ENABLED": true,
      "IOS_SYNCHRONOUSLY_UPDATE_UI_PROPS": true,
      "USE_SYNCHRONIZABLE_FOR_MUTABLES": true,
      "USE_COMMIT_HOOK_ONLY_FOR_REACT_COMMITS": false
    }
  },
  "dependencies": {
    "@expo-google-fonts/nunito": "^0.4.2",
    "@expo-google-fonts/space-mono": "^0.4.2",
    "@expo/config": "^12.0.10",
    "@hookform/resolvers": "^5.2.2",
    "@lodev09/react-native-true-sheet": "^2.0.6",
    "@radix-ui/colors": "^3.0.0",
    "@react-native-community/slider": "^5.0.1",
    "@react-native-segmented-control/segmented-control": "^2.5.7",
    "@react-navigation/native": "^7.1.6",
    "@shopify/react-native-skia": "2.2.12",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "expo": "~54.0.22",
    "expo-asset": "~12.0.9",
    "expo-build-properties": "~1.0.9",
    "expo-constants": "~18.0.10",
    "expo-dev-client": "~6.0.17",
    "expo-file-system": "~19.0.17",
    "expo-font": "~14.0.9",
    "expo-haptics": "^15.0.7",
    "expo-image": "~3.0.10",
    "expo-linking": "~8.0.8",
    "expo-router": "6.0.14",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.8",
    "expo-web-browser": "~15.0.9",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.65.0",
    "react-native": "0.81.5",
    "react-native-edge-to-edge": "^1.6.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-keyboard-controller": "^1.18.5",
    "react-native-nitro-modules": "^0.31.2",
    "react-native-reanimated": "~4.1.3",
    "react-native-safe-area-context": "5.6.2",
    "react-native-screens": "4.16.0",
    "react-native-svg": "^15.12.1",
    "react-native-unistyles": "3.0.16",
    "react-native-web": "~0.21.2",
    "react-native-worklets": "^0.5.1",
    "reanimated-color-picker": "^4.1.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.1.17",
    "@types/react-test-renderer": "^19.1.0",
    "babel-plugin-react-compiler": "^1.0.0",
    "babel-preset-expo": "^54.0.6",
    "eslint": "^9.38.0",
    "eslint-config-expo": "^10.0.0",
    "globals": "^16.4.0",
    "jest": "^29.7.0",
    "jest-expo": "54.0.13",
    "prettier": "^3.6.2",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.3"
  },
  "packageManager": "pnpm@10.20.0"
}
